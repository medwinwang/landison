<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd">

    <cache name="lps_token"
           maxEntriesLocalHeap="5"
           timeToLiveSeconds="1800">
    </cache>

    <cache name="kms_getCountry"
           maxEntriesLocalHeap="5"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="kms_getProvince"
           maxEntriesLocalHeap="100"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="kms_getCity"
           maxEntriesLocalHeap="100"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="kms_getInformation"
           maxEntriesLocalHeap="100"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="kms_getSingleHotelInfo"
           maxEntriesLocalHeap="100"
           timeToLiveSeconds="3600">
    </cache>

</ehcache>